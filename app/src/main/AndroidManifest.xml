<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="work.com.studyapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SecondActivity">
            <intent-filter>

                <!-- 给activity添加action -->
                <action android:name="work.com.studyapplication.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".RelativeActivity" />
        <activity android:name=".NormalActivity" />
        <activity android:name=".QQLoginActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".ListViewActivity"></activity>
    </application>

</manifest>